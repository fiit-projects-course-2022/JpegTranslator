class GigachadInt {
    field int type, value;

    constructor GigachadInt new(int v) {
        let value = v;
        let type = 1;
        return this;
    }

    constructor GigachadInt newChad(GigaChadInt v) {
        let value = v.GetValue(0);
        let type = 1;
        return this;
    }

    method int Type() { return type; }
    method int GetValue(int index) { return value; }
}